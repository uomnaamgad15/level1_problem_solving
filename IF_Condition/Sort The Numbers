#include <bits/stdc++.h>
using namespace std;
int main(){
    int a,b,c,x,y;
    cin>>a>>b>>c;
      if (a<=b && a<=c && b<=c)
          cout<<a<<" "<<b<<" "<<c;
      else if (b<=a && b<=c && a<=c)
          cout<<b<<" "<<a<<" "<<c;
      else if (c<=a && c<=b && a<=b)
          cout<<c<<" "<<a<<" "<<b;
      else if (c<=a && c<=b && b<=a)
          cout<<c<<" "<<b<<" "<<a;
      else if (a<=b && a<=c && c<=b)
          cout<<a<<" "<<c<<" "<<b;
      else
          cout<<b<<" "<<c<<" "<<a;

    
       //Another Solution
