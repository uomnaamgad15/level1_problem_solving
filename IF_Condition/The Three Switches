#include<bits/stdc++.h>
using namespace std;
int main() {
    bool a,b,c;
    int op;
    cin>>a>>b>>c>>op;
    if (op == 1) {
        a=a-1;
        cout<<a<<" "<<b<<" "<<c;
    }
    else if (op == 2) {
        b=b-1;
        cout<<a<<" "<<b<<" "<<c;
    }
    else if (op == 3) {
        c=c-1;
        cout<<a<<" "<<b<<" "<<c;
    }
    else if (op == 4) {
        a=a-1;
        b=b-1;
        c=c-1;
        cout<<a<<" "<<b<<" "<<c;
    }
    else if (op == 5) {
        if (a == 1)
            a=a-1;
        if (b == 1)
            b=b-1;
        if (c == 1)
            c=c-1;
        cout<<a<<" "<<b<<" "<<c;
    }
    return 0;
}
